<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC
		"-//Hibernate/Hibernate Configuration DTD 3.0//EN"
		"http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
    <session-factory name="ControleDispensacao">
        <property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>
        <property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
        <property name="hibernate.connection.url">jdbc:mysql://localhost/DBFarmacia</property>
        <property name="hibernate.connection.username">root</property>
        <property name="hibernate.connection.password">root</property>
        <mapping class="br.com.ControleDispensacao.entidade.Usuario"/>
        <mapping class="br.com.ControleDispensacao.entidade.Aplicacao"/>
        <mapping class="br.com.ControleDispensacao.entidade.Menu"/>
        <mapping class="br.com.ControleDispensacao.entidade.Fabricante"/>
        <mapping class="br.com.ControleDispensacao.entidade.TipoMaterial"/>
        <mapping class="br.com.ControleDispensacao.entidade.Grupo"/>
        <mapping class="br.com.ControleDispensacao.entidade.SubGrupo"/>
        <mapping class="br.com.ControleDispensacao.entidade.Familia"/>
        <mapping class="br.com.ControleDispensacao.entidade.Material"/>
        <mapping class="br.com.ControleDispensacao.entidade.UnidadeMaterial"/>
        <mapping class="br.com.ControleDispensacao.entidade.ListaEspecial"/>
        <mapping class="br.com.ControleDispensacao.entidade.Livro"/>
        <mapping class="br.com.ControleDispensacao.entidade.AutorizaAplicacao"/>
        <mapping class="br.com.ControleDispensacao.entidade.TipoConselho"/>
        <mapping class="br.com.ControleDispensacao.entidade.TipoProfissional"/>
        <mapping class="br.com.ControleDispensacao.entidade.LiberaMaterialTipoProfissional"/>
        <mapping class="br.com.ControleDispensacao.entidade.Profissional"/>
        <mapping class="br.com.ControleDispensacao.entidade.Estado"/>
        <mapping class="br.com.ControleDispensacao.entidade.Cidade"/>
        <mapping class="br.com.ControleDispensacao.entidade.Unidade"/>
        <mapping class="br.com.ControleDispensacao.entidade.AutorizaUnidadeProfissional"/>
        <mapping class="br.com.ControleDispensacao.entidade.SituacaoPaciente"/>
        <mapping class="br.com.ControleDispensacao.entidade.GrupoAtencaoContinuada"/>
    </session-factory>
</hibernate-configuration>
